<Window x:Class="Aspenlaub.Net.GitHub.CSharp.Backbend.GUI.BackbendWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Backbend" Height="400" Width="600" Background="LemonChiffon" Icon="filing.ico" Loaded="BackbendWindow_OnLoaded" Closing="BackbendWindow_OnClosing">
    <Grid>
        <WebBrowser
            x:Name="HtmlOutput" 
            Grid.Column="0" Grid.Row="0"
            Navigated="HtmlOutput_OnNavigated"
        />
    </Grid>
</Window>
